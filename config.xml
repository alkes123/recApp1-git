<?xml version="1.0" encoding="utf-8"?>

<widget xmlns="http://www.w3.org/ns/wigets"
xmlns:gap="http://phonegap.com/ns/1.0"
id="com.phonegap.example1"
version="1.0.0">

<name>app1 especialidad cbtis 16</name>

<description>
Aplicacion que muestra especialidades del cbtis 16
</description>

<author href="https://build.phonegap.com" email="aleksjoshuaa@gmail.com">
Aleks Joshua Aguilar Lopez
</author>

<!--personaliza la aplicacion plataforma -->
<preference name="orientation" value="default" />
<!--<preference name="orientation" value="portraint" />
<preference name="orientation" value="landscape" />-->
<preference name="target-device" value="universal" />
<preference name="fullscreen" value="true"/>
<preference name="android-installLocation" value="auto" />
<preference name="permissions" value="INTERNET"/>

<!--plugins-->
<!--core plugins-->
<plugin name="cordova-plugin-device" spec="~1.1.4"/>
<plugin name="cordova-plugin-vibration" spec="~2.1.3"/>
<plugin name="cordova-plugin-dialogs" spec="~1.3.1"/>

<!-- <plugin name="cordova-plugin-geolocalitation" source="npm"/>-->
<!-- Third party plugins -->

 
<!--Para accesar a paginas en internet-->
<allow-intent href="https://*/*"/>
<acces origin="https://*/*"/>
<allow-intent href="http://*/*"/>
<acces origin="http://*/*"/>
<plugin name="cordova-plugin-whitelist" source="npm"/>

<!-- Colocar icono app -->

<icon src="res/android/mdpi.png"/>

</widget>